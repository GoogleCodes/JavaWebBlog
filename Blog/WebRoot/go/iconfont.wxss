/* iconfont start */


@font-face {
	font-family: 'iconfont';
	src: url('iconfont.eot');
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'iconfont';
	src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
		url('iconfont.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-zan:before { content: "\e6ab"; }

.icon-sousuo:before { content: "\e606"; }

.icon-athit-iconfont-message:before { content: "\e626"; }

.icon-shijianfenzhong:before { content: "\e810"; }

/* iconfont end */